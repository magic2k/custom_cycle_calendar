<h1>Наши велосипеды</h1>

<!--rewrite with partials like "render partial: "bicycles", collection: @bicycle-->
<div class="row">
  <div class="span6 offset8">
    <div class="users">
      <ul>
        <% @bicycles.each do |bicycle| %>
            <li> <%= link_to(bicycle.name, bicycle) %> </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>